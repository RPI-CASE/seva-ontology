@prefix : <http://purl.org/seva/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix hasco: <http://hadatac.org/ont/hasco/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sdg: <http://purl.unep.org/sdg/> .
@base <http://purl.org/seva> .

<http://purl.org/seva>
        rdf:type owl:Ontology ;

        owl:imports <http://hadatac.org/ont/hasco/> ,
                    <http://purl.obolibrary.org/obo/envo.owl> ,
                    <http://semanticscience.org/ontology/sio-subset-labels.owl> ,
                    <http://semanticscience.org/ontology/sio.owl> .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://purl.org/seva/hasScale

:hasScale rdf:type owl:ObjectProperty ;

          rdfs:domain hasco:Project ;

          rdfs:range :Scale .

#################################################################
#
#    Classes
#
#################################################################


###  http://purl.org/seva/Building

:Building rdf:type owl:Class ;

          rdfs:subClassOf :Scale .



###  http://purl.org/seva/Micro

:Micro rdf:type owl:Class ;

       rdfs:subClassOf :Scale .



###  http://purl.org/seva/Nano

:Nano rdf:type owl:Class ;

      rdfs:subClassOf :Scale .



###  http://purl.org/seva/Room

:Room rdf:type owl:Class ;

      rdfs:subClassOf :Scale .



###  http://purl.org/seva/Scale

:Scale rdf:type owl:Class ;

       rdfs:subClassOf <http://ecoinformatics.org/oboe/oboe.1.0/oboe-core.owl#Characteristic> .



###  http://purl.org/seva/Urban

:Urban rdf:type owl:Class ;

       rdfs:subClassOf :Scale .

###########
#
#  Indicators
#
###########

###  http://purl.org/seva/PhysicalCharacteristicIndicator

:PhysicalCharacteristicIndicator a owl:Class ;
     rdfs:label "Physical Characteristic Indicator" ;
     rdfs:subClassOf hasco:StudyIndicator ;
     skos:definition "Physical Characteristic Indicator Class" .

###  http://purl.org/seva/ChemicalCharacteristicIndicator

:ChemicalCharacteristicIndicator a owl:Class ;
     rdfs:label "Chemical Characteristic Indicator" ;
     rdfs:subClassOf hasco:StudyIndicator ;
     skos:definition "Chemical Characteristic Indicator Class" .

###  http://purl.org/seva/SDGIndicator

:SDGIndicator a owl:Class ;
    rdfs:label "Sustainable Development Goal Indicator" ;
    rdfs:subClassOf hasco:StudyIndicator ;
    skos:definition "Sustainable Development Goal Indicator Class" .

obo:ENVO_00010483 a owl:Class ;
     rdfs:label "Environmental Material" ;
     rdfs:subClassOf  :PhysicalCharacteristicIndicator, sio:Specimen .


###########
#
#   classes
#
###########

###  http://purl.org/seva/DewPoint

:DewPoint a owl:Class;
     rdfs:label "dew point" ;
     skos:definition "The dew point is the temperature to which air must be cooled to become saturated with water vapor. [wikipedia: https://en.wikipedia.org/wiki/Dew_point]" ;
     rdfs:subClassOf  :ChemicalCharacteristicIndicator, sio:Specimen .

